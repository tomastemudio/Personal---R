det_2x2 <- function(x){
if (! is.matrix(x)){
stop("Debe ingresa una matriz.")
}else if (dim(x)[1] != dim(x)[2]){
stop("Debe ingresar una matriz cuadrada.")
}else if (dim(x)[1] != 2 && dim(x)[2] != 2){
stop('Debe ingresar una matriz de 2x2.')
}else{
cuenta <- (x[1,1] * x[2,2]) -(x[2,1] * x[1,2])
return(cuenta)
}
}
